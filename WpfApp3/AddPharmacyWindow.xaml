<Window x:Class="WpfApp3.AddPharmacyWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title="AddPharmacyWindow" Height="450" Width="800">
    <Grid>
        <StackPanel>
            <TextBox x:Name="NameTextBox" Text="Name"/>
            <TextBox x:Name="AddressTextBox" Text="Address"/>
            <TextBox x:Name="StartHourTextBox" Text="Start Hour"/>
            <TextBox x:Name="EndHourTextBox" Text="End Hour"/>
            <Button Content="Add" Click="AddButton_Click"/>
        </StackPanel>
    </Grid>
</Window>
